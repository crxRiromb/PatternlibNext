{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": ["vite/client"],
    "baseUrl": "."
  },
  "include": [".storybook/**/*", "src/**/*", "stories/**/*", "src/env.d.ts"],
  "exclude": ["node_modules", "dist"]
}
